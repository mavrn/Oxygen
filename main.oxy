fn fizz_buzz i => 
     for x=1, x<=i, x++ => 
             if x%3 == 0 and x%5 == 0 =>
                     out("FizzBuzz")
             <<
             or x%5 == 0 =>
                     out("Buzz")
             <<
             or x%3 == 0 =>
                     out("Fizz")
             <<
             else =>
                     out(x)
             <<
     <<
<<
10.fizz_buzz